<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>
        
        <PackageId>Framework.Jab</PackageId>

        <Title>Framework.Jab</Title>
        <Authors>byCrookie</Authors>
        <PackageTags>framework;boot;utilities;jab</PackageTags>
        <Description>jab boot base-framework for c# applications</Description>

        <RepositoryType>git</RepositoryType>
        <PackageProjectUrl>https://github.com/byCrookie/Framework.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/byCrookie/Framework.NET</RepositoryUrl>

        <Copyright>https://github.com/byCrookie/Framework.NET/blob/main/LICENSE</Copyright>
        <PackageLicenseExpression>BSD-3-Clause</PackageLicenseExpression>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="buildsdk" Version="1.0.2-pre-20220130032840" />
      <PackageReference Include="Jab" Version="0.6.3" />
      <PackageReference Include="workflow.jab" Version="1.0.2-pre-20220130033805" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Framework\Framework.csproj" />
    </ItemGroup>

</Project>
